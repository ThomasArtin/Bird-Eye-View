# create sensor mounts and cameras
left_fish_mount = visnodes.VisTransform.createTransformNode(r"left_fish_mount")
scene.BMW_X5_SUV_2.addChild(left_fish_mount)
left_fish_mount.setTranslation(1.15, 1, 1.10000000596046)
left_fish_mount.setRotationLegacyYUpAxes(2.3063665715257358, -0.633687274224642, -0.44371260627965781, 0.63368727422464211)

left_fish_monoBeam = visviewer.VisCamera(r"left_fish_monoBeam")
