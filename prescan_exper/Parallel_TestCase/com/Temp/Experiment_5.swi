left_fish_monoBeam.setRotationLegacyYUpAxes(1.5707963267948966, 0, 0, -1)
left_fish_monoBeam.setProjectionMatrixPerspective(0.80656297037058056, 0.61940592923487225, 0.10000000149011612, 750)

cameras[r"left_fish"] = VisCameras.CameraInfo(r"left_fish", left_fish_monoBeam, visviewer.VisViewerBase.SPECTATOR, True)
